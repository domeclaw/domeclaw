{
  "agents": {
    "defaults": {
      "model": "kimi-for-coding",
      "provider": "kimi-code",
      "workspace": "~/.domeclaw/workspace",
      "max_tool_iterations": 20,
      "max_tokens": 8192,
      "context_window": 100000,
      "temperature": 0.7,
      "restrict_to_workspace": true
    }
  },
  "model_list": [
    {
      "model_name": "kimi-for-coding",
      "model": "kimi-code/kimi-for-coding",
      "api_key": "YOUR_KIMI_CODE_API_KEY",
      "api_base": "https://api.kimi.com/coding/v1"
    },
    {
      "model_name": "qwen3.5-plus",
      "model": "qwen/qwen3.5-plus",
      "api_key": "YOUR_QWEN_CODE_API_KEY",
      "api_base": "https://coding-intl.dashscope.aliyuncs.com/v1"
    }
  ],
  "channels": {
    "telegram": {
      "enabled": false,
      "token": "YOUR_TELEGRAM_BOT_TOKEN"
    },
    "discord": {
      "enabled": false,
      "token": ""
    },
    "feishu": {
      "enabled": false,
      "app_id": "",
      "app_secret": ""
    },
    "slack": {
      "enabled": false,
      "token": ""
    },
    "whatsapp": {
      "enabled": false,
      "bridge_url": ""
    },
    "webhook": {
      "enabled": false,
      "token": "YOUR_WEBHOOK_TOKEN",
      "host": "0.0.0.0",
      "port": 18795
    }
  },
  "gateway": {
    "host": "0.0.0.0",
    "port": 8080,
    "http_api_port": 8081
  },
  "tools": {
    "web": {
      "brave": {
        "enabled": false,
        "api_key": "",
        "max_results": 5
      },
      "tavily": {
        "enabled": false,
        "api_key": ""
      }
    },
    "skills": {
      "max_concurrent_searches": 3
    },
    "cron": {
      "exec_timeout_minutes": 5
    }
  },
  "session": {
    "dm_scope": "cli"
  },
  "heartbeat": {
    "enabled": false,
    "interval": 300
  },
  "devices": {
    "enabled": false,
    "monitor_usb": false
  },
  "wallet": {
    "enabled": true,
    "chains": [
      {
        "name": "ClawSwift",
        "chain_id": 7441,
        "rpc": "https://exp.clawswift.net/rpc",
        "explorer": "https://exp.clawswift.net",
        "currency": "CLAW",
        "is_native": false,
        "gas_token": "0x20c0000000000000000000000000000000000000",
        "gas_token_name": "CLAW"
      }
    ]
  }
}
